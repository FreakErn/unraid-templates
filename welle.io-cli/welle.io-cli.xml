<?xml version="1.0"?>
<Container version="2">
  <Name>welle.io-cli</Name>
  <Repository>freakern/welle.io</Repository>
  <Registry>https://hub.docker.com/r/freakern/welle.io</Registry>
  <Network>bridge</Network>
  <MyIP/>
  <Shell>sh</Shell>
  <Privileged>false</Privileged>
  <Support/>
  <Project>https://github.com/albrechtl/welle.io#welle-cli</Project>
  <Overview>welle.io is an open source DAB and DAB+ software defined radio (SDR) with support for rtl-sdr (RTL2832U) and airspy.</Overview>
  <Category>MediaServer:Music</Category>
  <WebUI>http://[IP]:[PORT:8000]</WebUI>
  <TemplateURL/>
  <Icon>https://www.welle.io/assets/img/favicon-32x32.png</Icon>
  <ExtraParams/>
  <PostArgs>-F rtl_sdr -w 8000</PostArgs>
  <CPUset/>
  <DateInstalled>1675766632</DateInstalled>
  <DonateText/>
  <DonateLink/>
  <Requires/>
  <Config Name="Streaming Port" Target="8000" Default="" Mode="tcp" Description="" Type="Port" Display="always" Required="false" Mask="false">8000</Config>
  <Config Name="USB bus folder (For automatically selection of the RTL2832U)" Target="/dev/bus/usb" Default="/dev/bus/usb" Mode="rw" Description="" Type="Path" Display="always" Required="false" Mask="false">/dev/bus/usb</Config>
</Container>